{
    "compilerOptions": {
        "target": "es6",
        "module": "CommonJS",
        "outDir": "./dist",
        "rootDir": ".",
        "strict": true,
        "strictPropertyInitialization": false,
        "moduleResolution": "node",
        "noImplicitAny": false,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "typeRoots": ["./node_modules/@types", "./src/@types/project"]
    },
    "exclude": ["node_modules"]
}
